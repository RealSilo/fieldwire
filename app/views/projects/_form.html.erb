<%= form_for [current_user, project] do |f| %>
  <%= render 'layouts/error_messages', object: f.object %>

  <div class="row">
      <div class="col-md-3">
        <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control' %>
      </div>
    </div>
  </div>

  <div class="row">
      <div class="col-md-12">
        <div class="form-group">
          <%= f.submit 'Create project' ,class: "btn btn-primary" %>
        </div>
      </div>
    </div>
<% end %>
