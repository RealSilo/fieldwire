<h2>Your projects</h2>

<%= link_to 'New Project', new_user_project_path, class: "btn btn-primary" %>

<% @projects.each do |project| %>
  <div class="row" style="padding-top:15px;">
    <div class="col-md-12">
      <%= link_to user_project_path(current_user, project) do %>
        <%= project.name %>
      <% end %>
    </div>
  </div>
<% end %>
